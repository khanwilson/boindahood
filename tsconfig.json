{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "app/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/constants/*": [
        "constants/*"
      ],
      "@/hooks/*": [
        "hooks/*"
      ],
      "@/services/*": [
        "services/*"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/assets/*": [
        "assets/*"
      ],
      "@/context/*": [
        "context/*"
      ],
      "@/shared/*": [
        "context/*"
      ],
    }
  },
  "include": [
    "app/**/*",
    "components/**/*",
    "constants/**/*",
    "hooks/**/*",
    "services/**/*",
    "types/**/*",
    "assets/**/*",
    "context/**/*",
    "shared/**/*",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
